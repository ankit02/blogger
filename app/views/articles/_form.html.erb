<%= form_for(@article) do |f| %>
	<ul>
		<% @article.errors.full_messages.each do |error| %>
		<l1><%= error %></l1>
		<% end %>
	</ul>
	<p>
		<%= f.label :title %><br/>
		<%= f.text_field :title %><br/>
	</p>
	<p>
		<%= f.label :body %><br/>
		<%= f.text_field :body %><br/>
	</p>
	<p>
		<%= f.submit %>
	</p>
	<% end %>